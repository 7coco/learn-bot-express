{
  "extends": "airbnb",
  "rules":{
    "key-spacing":["error",{"afterColon":true}],
    "import/no-extraneous-dependencies":0,
    "max-len":0,
    "no-var":0,
    "vars-on-top":0,
    "no-continue":0,
    "prefer-const":0,
    "newline-per-chained-call":0,
    "keyword-spacing":0,
    "space-before-blocks":0,
    "no-param-reassign":0,
    "prefer-template":0,
    "no-useless-escape":0,
    "strict":0,
    "indent":["error", 4],
    "quotes":["error", "double"],
    }
}
